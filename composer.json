{
  "name": "composer-github-hook",
  "description": "This is a hook",
  "require": {
    "marktopper/composer-hook-dependency-1": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ComposerGithubHook\\": "src/"
    }
  },
  "extra": {
    "hook": {
      "providers": [
        "ComposerGithubHook\\ComposerGithubHookServiceProvider"
      ],
      "aliases": {
        "Test": "ComposerGithubHook\\TestAlias"
      }
    }
  }
}
